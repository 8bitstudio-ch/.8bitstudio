{
  "semi": false,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "singleAttributePerLine": true,
  "printWidth": 80,
  "plugins": ["prettier-plugin-css-order"],
  "cssDeclarationSorterOrder": "smacss"
}
